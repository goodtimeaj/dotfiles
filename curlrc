# Don't show progress meter or error messages...
silent

# ...Unless it fails
show-error

# Follow 3XX response code redirects
location

# When following a redirect, automatically set the previous URL as the referer
referer = ";auto"

# Scan any `$HOME/.netrc` for login name and password
netrc

# Disguise as IE 9 on Windows 7
user-agent = "Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)"
